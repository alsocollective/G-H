{% assign collection = collections["featured-products"] %}


<div class="featureditems">


{% for product in collection.products %}{% if product.title != productName %}<div class="product">

	<div class="backgroundimage" style="background-image:url({{ product.featured_image | product_img_url: 'grande' }})"></div>
	<div class="text" href="#">
		<h3>{{product.title}}</h3>
		{% include "product-get-colours" %}
		<div class="bottom">
			{{product.content}}
		</div>
	</div>
	{% assign productName = product.title %}

</div>{% endif %}{% endfor %}

</div>